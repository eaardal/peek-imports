{
  "name": "peeky",
  "displayName": "Peeky",
  "description": "Peek and modify import statements without leaving the line and cursor position you're working on.",
  "publisher": "eaardal",
  "icon": "./peeky.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/eaardal/peeky"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "peeky.peekImports",
        "title": "Peeky: Peek Imports"
      }
    ],
    "keybindings": [
      {
        "command": "peeky.peekImports",
        "key": "ctrl+i",
        "mac": "ctrl+i",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "local-install": "code --install-extension peeky-0.0.1.vsix"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vsce": "^1.95.0",
    "vscode-test": "^1.5.2"
  }
}
